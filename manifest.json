{
  "name": "YouTube Ad Skipper",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Automajically skip youtube ads",
  "homepage_url": "http://onflo.io",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*",
        "http://www.youtube.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}